{
  "name": "@core/github-cli",
  "version": "0.0.2",
  "description": "https://github.com/chlbri/github-cli.git",
  "author": "chlbri <bri_lvi@icloud.com>",
  "license": "MIT",
  "private": true,
  "bin": {
    "@github-cli/commit": "bin/cli/commit.js",
    "commit": "bin/cli/commit.js"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "pnpm test && pnpm build:-test",
    "build:-test": "pnpm tsc && pnpm lint",
    "clear:test": "jest --clearCache",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx,yml,json}\"",
    "lint": "pnpm prettier && pnpm eslint -- --fix",
    "rm": "rimraf -rf node_modules pnpm-lock.yaml",
    "rinit": "pnpm rm && pnpm pnpm",
    "publish": "pnpm run rm && pnpm run clean && pnpm run pnpm",
    "pnpm": "pnpm install",
    "upgrade": "pnpm upgrade",
    "upgrade-internal": "pnpm upgrade core core-promises",
    "git-push": "git push --follow-tags",
    "git-build-push": "pnpm pnpm && pnpm build && pnpm git-push",
    "git-publish": "pnpm git-build-push && git checkout main && git merge dev && pnpm git-push && git checkout dev"
  },
  "devDependencies": {
    "@types/concat-stream": "^1.6.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/esm": "^3.2.0",
    "@types/faker": "^5.5.9",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^27.0.3",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "core-test": "github:chlbri/core-test#v0.0.13",
    "dayjs": "^1.10.7",
    "edit-json-file": "^1.6.2",
    "eslint": "^8.3.0",
    "faker": "^5.5.3",
    "immer": "^9.0.6",
    "jest": "^27.3.1",
    "jest-cucumber": "^3.0.1",
    "jest-extended": "^1.1.0",
    "object.omit": "^3.0.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "arg": "^5.0.1",
    "concat-stream": "^2.0.0",
    "core": "github:chlbri/core#v0.0.22",
    "core-test": "github:chlbri/core-test",
    "cross-spawn": "^7.0.3",
    "dequal": "^2.0.2",
    "esm": "^3.2.25",
    "gluegun": "^4.7.0",
    "inquirer": "^8.2.0",
    "mock-stdin": "^1.0.0",
    "nanoid": "^3.1.30",
    "shelljs": "^0.8.4",
    "zod": "^3.11.6"
  },
  "keywords": [
    "cli",
    "github",
    "git",
    "github-cli",
    "@core"
  ],
  "repository": "https://github.com/chlbri/github-cli.git"
}
